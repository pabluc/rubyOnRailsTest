
<h1>Listing properties</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th>Prices</th>
  </tr>
 
  <% @properties.each do |property| %>
    <tr>
      <td><%= property.title %></td>
      <td><%= property.text %></td>
      <td><%= property.price %></td>
      <td><%= link_to 'Show', property_path(property) %></td>
    </tr>
  <% end %>

</table>

<%= link_to 'New', new_property_path %>